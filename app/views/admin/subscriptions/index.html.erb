<h1>Listing admin_subscriptions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_subscriptions.each do |admin_subscription| %>
      <tr>
        <td><%= admin_subscription.name %></td>
        <td><%= admin_subscription.code %></td>
        <td><%= link_to 'Show', admin_subscription %></td>
        <td><%= link_to 'Edit', edit_admin_subscription_path(admin_subscription) %></td>
        <td><%= link_to 'Destroy', admin_subscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscription', new_admin_subscription_path %>
